            <Button fx:id="showDiagramButton" mnemonicParsing="false" onAction="#showDiagram" text="PokaÅ¼ diagram" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <Label text="Data:" GridPane.rowIndex="2" />
            <TextField fx:id="dateTextField" promptText="dd/mm/rr" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </center>
   <top>
      <HBox prefHeight="20.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Pacjent:">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="patientLabel">
               <HBox.margin>
                  <Insets bottom="10.0" left="5.0" right="5.0" />
