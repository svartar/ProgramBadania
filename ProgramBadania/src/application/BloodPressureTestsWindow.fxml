         </rowConstraints>
         <children>
            <Label text="Wysokie:" />
            <Label text="Niskie:" GridPane.rowIndex="1" />
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonAction" text="Zapisz" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="highTextField" GridPane.columnIndex="1" />
            <TextField fx:id="lowTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="messageLabel" GridPane.columnIndex="1" GridPane.rowIndex="3">
               <GridPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
